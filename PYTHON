KAMIEN PAPIER NOZYCE 

import random
print("gra papier kamien nozyce")
print("zaczynamy gre")
komputer = random.randit(1,3)
print(komputer)
komputer = str(komputer)
print("wybierz jedno (1) papier (2) kamien (3) nozyce?")
while ask not in ("1", "2", "3"):
  print("wybierz poprawy ruch")
  ask = input

ask = int(ask)
#ask = input
#if ask > 3:
 # print("wybierz ponownie")
#else:
  #print("teraz wybierze komputer")
if ask == 1 and komputer == 1:
  print("remis")
elif ask == 2 and komputer == 2:
  print("remis!")
elif ask == 3 and komputer == 3:
  print("remis")
elif ask == 2 and komputer == 1:
  print("przegrales")
elif ask == 2 and komputer == 3:
  print("wygrales")
elif ask == 3 and komputer == 2:
  print("wygrales")
elif ask == 1 and komputer == 3:
  print("przegrales")
elif ask == 3 and komputer == 1:
  print("wygrales")
elif ask == 3 and komputer == 2:
  print("przegrales")



