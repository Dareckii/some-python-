ZAGDYWANKA PLANETY

import random
planety = ["merkury", "wenus", "ziemia", "mars", "jowisz", "saturn", "uran", "neptune"]
x = random.sample(planety,1)
if "ziemia" in x:
  print("jest zielona i mokra")
if "merkury" in x:
  print("jest malutka")
if "wenus" in x:
  print("stamtąd sa kobiety")
if "mars" in x:
  print("stamtąd sa mężczyźni")
if "jowisz" in x:
  print("jest najwieksza w układzie słonecznym")
if "saturn" in x:
  print("taki sklep z agd")
if "uran" in x:
  print("robimy z niego bomby")
if "neptune" in x:
  print("cała niebieska")
  
print("co to za planeta, zgadnij")
lista = input()
if "ziemia" in lista:
  print("dobrze to ziemia")
if "merkury" in lista:
  print("tak to merkury")
if "wenus" in lista:
  print("tak to wenus")
if "jowisz" in lista:
  print("tak to jowisz")
if "saturn" in lista:
  print("tak to saturn")
if "uran" in lista:
  print("tak to uran")
if "neptune" in lista:
  print("tak to neptun")
if not in planety:
  print("pomylka jeszcze dwie proby")





  
